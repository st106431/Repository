#include <iostream>
#include <algorithm>
using namespace std;

char ask(int i, int j)
{
	char y;
	cout << "? " << i << " " << j << endl;
	fflush(stdout);
	cin >> y;
	return y;
}

void small_sort(int* buff, int k)
{
	int* temp = new int[k] {0};
	int* copy = new int[k];
	for (int i = 0; i < k; i++)
	{
		copy[i] = buff[i];
	}
	for (int i = 0; i < k; i++)
	{
		for (int j = 0; j < k; j++)
		{
			if (i == j)
			{
				temp[i]++;
			}
			else
			{
				if (ask(buff[i], buff[j]) == '>')
				{
					temp[i]++;
				}
			}
		}
	}
	for (int i = 0; i < k; i++)
	{
		buff[temp[i] - 1] = copy[i];
	}
}

int bin(int* buff, int i, int k, int l, int r)
{
	if (l == r)
	{
		if (ask(buff[l], i) == '>')
		{
			return l;
		}
		else
		{
			return l + 1;
		}
	}
	if (r - l == 1)
	{
		if (ask(buff[r], i) == '>')
		{
			return r;
		}
		else
		{
			return r + 1;
		}
	}
	if (ask(buff[(l + r) / 2], i) == '>')
	{
		return bin(buff, i, k, l, (l + r) / 2);
	}
	else
	{
		return bin(buff, i, k, (l + r) / 2, r);
	}
}

int main()
{
	int n, k;
	cin >> n >> k;
	int* buff = new int[k];
	for (int i = 0; i < k; i++)
	{
		buff[i] = i + 1;
	}
	small_sort(buff, k);
	for (int i = k; i < n; i++)
	{
		buff[bin(buff, i + 1, k, 0, k - 1)] = i + 1;
	}
	cout << "!";
	for (int i = 0; i < k; i++)
	{
		cout << " " << buff[i];
	}
}
