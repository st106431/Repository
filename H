#include <iostream>
using namespace std;

int main()
{
	int count = 0;
	int temp;
	int k;
	int l = 1;
	int r = 1000000000;
	while ((l < r) && (count < 100))
	{
		cin >> temp;
		count++;
		k = (l + r) / 2;
		if ((l <= temp) && (temp<= r))
		{
			if (temp <= k)
			{
				cout << ">" << endl;
				fflush(stdout);
				l = temp + 1;
			}
			else
			{
				cout << "<" << endl;
				fflush(stdout);
				r = temp - 1;
			}
		}
		else
		{
			if (temp < l)
			{
				cout << ">" << endl;
				fflush(stdout);
			}
			if (temp > r)
			{
				cout << "<" << endl;
				fflush(stdout);
			}
		}
	}
	if (count < 100)
	{
		cin >> temp;
		if (temp == l)
		{
			cout << "=";
		}
	}
}
